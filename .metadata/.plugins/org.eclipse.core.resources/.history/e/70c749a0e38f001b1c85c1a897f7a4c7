#ifndef LED_H_
#define LED_H_

#define DELAY_1S (PROCESSOR_CLOCK_SPEED)
#define DELAY_500MS (DELAY_1S/2)
#define DELAY_250MS (DELAY_1S/4)
#define DELAY_125MS (DELAY_1S/8)

#define GREEN_LED 	12
#define ORANGE_LED 	13
#define RED_LED 	14
#define BLUE_LED 	15

#endif /* LED_H_ */
